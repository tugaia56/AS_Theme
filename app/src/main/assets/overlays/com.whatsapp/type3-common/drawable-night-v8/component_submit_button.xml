<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="15.0dip" />
            <stroke android:width="1.5dip" android:color="@*android:color/accent_material_dark" />
            <gradient android:startColor="@null" android:endColor="@null" android:angle="-90.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="15.0dip" />
            <stroke android:width="1.5dip" android:color="@*android:color/accent_material_dark" />
            <solid android:color="@null" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="15.0dip" />
            <stroke android:width="1.5dip" android:color="@*android:color/accent_material_dark" />
            <gradient android:startColor="@*android:color/accent_material_dark" android:endColor="@*android:color/accent_material_dark" android:angle="-90.0" />
        </shape>
    </item>
</selector>